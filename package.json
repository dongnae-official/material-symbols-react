{
  "name": "@dongnae/material-symbols-react",
  "version": "1.25.6",
  "description": "Flexible React package based on Google's Material Icons",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dongnae-official/material-symbols-react.git"
  },
  "exports": {
    "./outlined": "./dist/esm/outlined/index.js",
    "./outlined/filled": "./dist/esm/outlined/filled/index.js",
    "./rounded": "./dist/esm/rounded/index.js",
    "./rounded/filled": "./dist/esm/rounded/filled/index.js",
    "./sharp": "./dist/esm/sharp/index.js",
    "./sharp/filled": "./dist/esm/sharp/filled/index.js"
  },
  "scripts": {
    "prepare": "husky install",
    "check": "bun run download:metadata -- --status --dry-run",
    "update": "bun run generate && bun run build",
    "download": "bun run download:metadata",
    "download:font": "bunx --yes @material-design-icons/scripts download font --to iconfont",
    "download:metadata": "bunx @material-design-icons/scripts download metadata --symbols",
    "generate": "bun ./src/generate.ts",
    "clean": "rimraf dist",
    "prebuild": "bun run clean",
    "build": "node --max-old-space-size=8192 node_modules/rollup/dist/bin/rollup -c",
    "build:min": "MINIFY=true node --max-old-space-size=8192 node_modules/rollup/dist/bin/rollup -c",
    "build:fast": "SKIP_TYPES=true node --max-old-space-size=8192 node_modules/rollup/dist/bin/rollup -c",
    "build:fast:min": "MINIFY=true SKIP_TYPES=true node --max-old-space-size=8192 node_modules/rollup/dist/bin/rollup -c",
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "prettier": "prettier --write \"{src,example/src}/**/*.{js,ts,jsx,tsx}\""
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "react",
    "react-component",
    "mui",
    "material-ui",
    "material design",
    "icons"
  ],
  "author": "dongnae-p",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/dongnae-official/material-symbols-react/issues"
  },
  "homepage": "https://github.com/dongnae-official/material-symbols-react#readme",
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.5.2",
    "parcel": "^2.16.3",
    "prettier": "^3.8.0",
    "puppeteer": "^24.35.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^5.0.10",
    "rollup": "^4.55.1",
    "rollup-plugin-dts": "^6.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "react": ">=18.2.0"
  }
}
